{
  "name": "aeron-bep2e",
  "version": "1.0.0",
  "description": "Aeron BEP2E Contracts and Staking DApp",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "css": "tailwindcss build src/components/inc/App.scss -o src/components/inc/App.css",
    "dev": "truffle deploy --network development",
    "tbsc": "truffle deploy --network tbsc",
    "test": "truffle test --network test",
    "coverage": "solidity-coverage",
    "start": "npm run css && react-scripts start",
    "build": "npm run css && react-scripts build",
    "test:dapp": "npm run css && react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "https://aeron.aero/staking",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@openzeppelin/contracts": "^3.0.2",
    "identicon.js": "^2.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "tailwindcss": "^1.4.6",
    "web3": "^1.2.9"
  },
  "devDependencies": {
    "ganache-cli": "^6.9.1",
    "solidity-coverage": "^0.7.*",
    "truffle": "^5.1.29"
  }
}
